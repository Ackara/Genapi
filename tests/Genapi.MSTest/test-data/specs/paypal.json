{
	"$schema": "https://raw.githubusercontent.com/OAI/OpenAPI-Specification/main/schemas/v3.1/schema.json",
	"openapi": "3.0.3",
	"info":
	{
		"title": "Swagger Petstore - OpenAPI 3.0",
		"version": "2024.5.28"
	},
	"servers":
	[
		{
			"url": "https://api-m.sandbox.paypal.com/v1"
		}
	],
	"securitySchemes":
	{

	},
	"paths":
	{
	},
	"components":
	{
		"schemas":
		{
			"AccessToken":
			{
				"type": "object",
				"properties":
				{
					"scope":
					{
						"type": "string"
					},
					"access_token":
					{
						"type": "string"
					},
					"token_type":
					{
						"type": "string"
					},
					"app_id":
					{
						"type": "string"
					},
					"expires_in":
					{
						"type": "integer"
					},
					"nonce":
					{
						"type": "string"
					}
				}
			}
		},
		"securitySchemes":
		{
			"petstore_auth":
			{
				"type": "oauth2",
				"flows":
				{
					"implicit":
					{
						"authorizationUrl": "https://petstore3.swagger.io/oauth/authorize",
						"scopes":
						{
							"write:pets": "modify pets in your account",
							"read:pets": "read your pets"
						}
					}
				}
			},
			"api_key":
			{
				"type": "apiKey",
				"name": "api_key",
				"in": "header"
			}
		}
	}
}